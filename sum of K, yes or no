#include<iostream>
#include<vector>
using namespace std;
int main() {
	int n, k;
	while (cin >> n >> k) {
		vector<int>a(n);
		for (int i = 0; i < n; i++) {
			cin >> a[i];
		}
		for (int i = 0; i < n-1; i++) {
			for (int j = i + 1; j < n; j++) {
				if (a[i] + a[j] == k) {
					cout << "yes" << endl;
					return 1;
				}
			}
		}
		
	}
	system("pause");
	return 0;
}
